{
  "name": "@lucca-front/demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-icons": "mkdirp ./dist/icons/font && cp -r ../icons/font/* ./dist/icons/font",
    "build-scss": "cd ../scss && gulp dist",
    "build-ng": "cd ../ng && gulp generatedoc && ng build -a=demo --base-href /ng/",
    "postbuild-scss": "mkdirp ./dist/scss && cp -r ../scss/demo/* ./dist/scss/ && cp -r ../scss/dist/* ./dist/scss/",
    "postbuild-ng": "mkdirp ./dist/ng && cp -r ../ng/demo-dist/* ./dist/ng/",
    "build-demo": "cp -r src/* ./dist/",
    "build": "npm run build-icons && npm run build-scss && npm run build-ng && npm run build-demo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
