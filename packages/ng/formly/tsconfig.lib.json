/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../out-tsc/lib",
    "target": "es2015",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "es2018"
    ],
    "paths": {
      "@lucca-front/ng/user": [
        "./dist/ng/user"
      ],
      "@lucca-front/ng/department": [
        "./dist/ng/department"
      ],
      "@lucca-front/ng/api": [
        "./dist/ng/api"
      ],
      "@lucca-front/ng/establishment": [
        "./dist/ng/establishment"
      ],
      "@lucca-front/ng/qualification": [
        "./dist/ng/qualification"
      ],
      "@lucca-front/ng/date": [
        "./dist/ng/date"
      ],
      "@lucca-front/ng/select": [
        "./dist/ng/select"
      ],
      "@lucca-front/ng/input": [
        "./dist/ng/input"
      ],
      "@lucca-front/ng/option": [
        "./dist/ng/option"
      ],
      "@lucca-front/ng/core": [
        "./dist/ng/core"
      ],
      "@lucca-front/ng/picker": [
        "./dist/ng/picker"
      ],
      "@lucca-front/ng/popover": [
        "./dist/ng/popover"
      ]
    }
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
