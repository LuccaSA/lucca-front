{
  "name": "@lucca-front/ng-internal",
  "version": "2.5.0",
  "description": "a library of usefull components for angular web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuccaSA/lucca-front.git"
  },
  "private": true,
  "keywords": [
    "lucca",
    "angular",
    "ng"
  ],
  "author": "lucien bertin <lbertin@lucca.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuccaSA/lucca-front/issues"
  },
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "prestart_old": "npm run generatedoc",
    "start": "ng serve sandbox",
    "build": "npm run build:lib",
    "test": "ng test core",
    "test:sr": "ng test core --watch=false",
    "lint": "ng lint",
    "gulp": "gulp",
    "generatedoc": "gulp generatedoc",
    "pretravis": "npm run build:lib",
    "travis": "npm run build:aot",
    "posttravis": "npm run build:demo",
    "prebuild:demo": "npm run generatedoc",
    "build:demo": "ng build demo",
    "prebuild:aot": "npm run generatedoc",
    "build:aot": "ng build demo --prod",
    "build:core": "ng build core",
    "postbuild:core": "gulp copy:core-style",
    "build:material": "ng build material",
    "postbuild:material": "gulp copy:material-style",
    "build:formly": "ng build formly",
    "postbuild:formly": "gulp copy:formly-style",
    "build:lib": "npm run build:core && npm run build:material && npm run build:formly",
    "build:schematics": "tsc --project schematics",
    "postbuild:schematics": "npm run link:schematics",
    "link:schematics": "cd schematics && npm link",
    "postlink:schematics": "npm link @lucca-front/schematics"
  },
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^7.2.11",
    "@angular/forms": "^7.2.11",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@lucca-front/icons": "2.5.0",
    "@lucca-front/scss": "2.5.0",
    "@ngx-formly/core": "^4.8.2",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.11",
    "@angular/material-moment-adapter": "^7.3.6",
    "@angular-devkit/schematics": "^7.3.8",
    "@angular-devkit/schematics-cli": "^0.13.8",
    "@angular/router": "^7.2.11",
    "@types/jasmine": "3.3.0",
    "@types/node": "^10.12.10",
    "codelyzer": "^4.5.0",
    "css-vars-ponyfill": "^1.17.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "merge2": "^1.2.3",
    "moment": "^2.22.2",
    "ng-packagr": "^4.7.0",
    "node-sass": "^4.10.0",
    "prismjs": "^1.15.0",
    "prismjs-loader": "0.0.4",
    "protractor": "^5.4.1",
    "run-sequence": "^2.2.1",
    "ts-node": "~7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "3.2.2"
  }
}
