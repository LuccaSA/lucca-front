{
  "name": "@lucca-front/ng-internal",
  "version": "4.0.0-placeholder",
  "description": "a library of usefull components for angular web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuccaSA/lucca-front.git"
  },
  "private": true,
  "keywords": [
    "lucca",
    "angular",
    "ng"
  ],
  "author": "lucien bertin <lbertin@lucca.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuccaSA/lucca-front/issues"
  },
  "scripts": {
    "ng": "ng",
    "cpx": "cpx",
    "tsc": "tsc",
    "start": "ng serve sandbox",
    "build": "npm run build:lib",
    "test": "ng test core",
    "test:sr": "ng test core --watch=false",
    "lint": "ng lint",
    "build:publish": "ng build sandbox --prod --output-path ../../demo/ng",
    "build:lib": "npm run build:depth0 && npm run build:depth1 && npm run build:depth2 && npm run build:depth3 && npm run build:depth4",
    "build:depth0": "npm run ng build core && npm run ng build animations",
    "build:depth1": "npm run ng build input && npm run ng build scroll && npm run ng build safe-content && npm run ng build number && npm run ng build material && npm run ng build popover && npm run ng build popup",
    "build:depth2": "npm run ng build tooltip && npm run ng build picker && npm run ng build modal",
    "build:depth3": "npm run ng build select && npm run ng build sidepanel && npm run ng build option",
    "build:depth4": "npm run ng build api && npm run ng build department && npm run ng build user",
    "build:schematics": "tsc --project schematics",
    "postbuild:schematics": "npm run link:schematics",
    "link:schematics": "cd schematics && npm link",
    "postlink:schematics": "npm link @lucca-front/schematics",
    "issue": "ng g @lucca-front/schematics:issue"
  },
  "dependencies": {
    "@angular/animations": "^8.2.9",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "^8.2.9",
    "@angular/compiler": "^8.2.9",
    "@angular/core": "^8.2.9",
    "@angular/forms": "^8.2.9",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "^8.2.9",
    "@angular/platform-browser-dynamic": "^8.2.9",
    "@lucca-front/scss": "4.0.0-placeholder",
    "@lucca-front/icons": "4.0.0-placeholder",
    "@ngx-formly/core": "^5.4.3",
    "core-js": "^3.2.1",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.8",
    "@angular-devkit/build-ng-packagr": "^0.803.8",
    "@angular/cli": "^8.3.8",
    "@angular/compiler-cli": "^8.2.9",
    "@angular/material-moment-adapter": "^8.2.2",
    "@angular-devkit/schematics": "^8.3.8",
    "@angular-devkit/schematics-cli": "^0.803.8",
    "@angular/router": "^8.2.9",
    "@types/jasmine": "3.4.2",
    "@types/node": "^12.7.11",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "moment": "^2.24.0",
    "ng-packagr": "^5.3.0",
    "protractor": "^5.4.1",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3"
  }
}
