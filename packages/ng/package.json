{
  "name": "@lucca-front/ng-internal",
  "version": "3.0.0-beta.1",
  "description": "a library of usefull components for angular web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuccaSA/lucca-front.git"
  },
  "private": true,
  "keywords": [
    "lucca",
    "angular",
    "ng"
  ],
  "author": "lucien bertin <lbertin@lucca.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuccaSA/lucca-front/issues"
  },
  "scripts": {
    "ng": "ng",
    "cpx": "cpx",
    "tsc": "tsc",
    "start": "ng serve sandbox",
    "build": "npm run build:lib",
    "test": "ng test core",
    "test:sr": "ng test core --watch=false",
    "lint": "ng lint",
    "gulp": "gulp",
    "pretravis": "npm run build:lib",
    "travis": "npm run build:aot",
    "build:aot": "ng build sandbox --prod",
    "build:core": "ng build core",
    "lolpostbuild:core": "gulp copy:core-style",
    "postbuild:core": "cpx libraries/core/src/style/** dist/style",
    "build:material": "ng build material",
    "postbuild:material": "cpx libraries/material/src/style/** dist/material/style",
    "build:formly": "ng build formly",
    "postbuild:formly": "cpx libraries/formly/src/style/** dist/formly/style",
    "build:lib": "npm run build:core && npm run build:material && npm run build:formly",
    "build:schematics": "tsc --project schematics",
    "postbuild:schematics": "npm run link:schematics",
    "link:schematics": "cd schematics && npm link",
    "postlink:schematics": "npm link @lucca-front/schematics"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0-rc.3",
    "@angular/cdk": "^8.0.0-rc.1",
    "@angular/common": "^8.0.0-rc.3",
    "@angular/compiler": "^8.0.0-rc.3",
    "@angular/core": "^8.0.0-rc.3",
    "@angular/forms": "^8.0.0-rc.3",
    "@angular/material": "^8.0.0-rc.1",
    "@angular/platform-browser": "^8.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^8.0.0-rc.3",
    "@lucca-front/scss": "3.0.0-beta.1",
    "@lucca-front/icons": "3.0.0-beta.1",
    "@ngx-formly/core": "^4.8.2",
    "core-js": "^2.5.7",
    "rxjs": "^6.5.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.0-rc.3",
    "@angular-devkit/build-ng-packagr": "^0.800.0-rc.3",
    "@angular/cli": "^8.0.0-rc.3",
    "@angular/compiler-cli": "^8.0.0-rc.3",
    "@angular/material-moment-adapter": "^8.0.0-rc.1",
    "@angular-devkit/schematics": "^8.0.0-rc.3",
    "@angular-devkit/schematics-cli": "^0.800.0-rc.3",
    "@angular/router": "^8.0.0-rc.3",
    "@types/jasmine": "3.3.0",
    "@types/node": "^10.12.10",
    "codelyzer": "^4.5.0",
    "cpx": "^1.5.0",
    "css-vars-ponyfill": "^1.17.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.22.2",
    "ng-packagr": "^5.1.0",
    "node-sass": "^4.12.0",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "3.4.5"
  }
}
