{
	"name": "@lucca-front/ng-internal",
	"version": "5.0.1",
	"description": "a library of usefull components for angular web app",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LuccaSA/lucca-front.git"
	},
	"private": true,
	"keywords": [
		"lucca",
		"angular",
		"ng"
	],
	"author": "lucien bertin <lbertin@lucca.fr>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/LuccaSA/lucca-front/issues"
	},
	"scripts": {
		"ng": "ng",
		"cpx": "cpx",
		"tsc": "tsc",
		"start": "ng serve sandbox",
		"ie11": "ng serve sandbox --configuration es5",
		"build": "npm run build:lib",
		"test": "ng test core",
		"test:sr": "ng test core --watch=false",
		"lint": "ng lint",
		"build:publish": "ng build sandbox --prod --output-path ../../demo/ng",
		"build:lib": "npm run build:root && npm run build:depth0 && npm run build:depth1 && npm run build:depth2 && npm run build:depth3 && npm run build:depth4 && npm run build:formly && npm run build:material",
		"build:depth0": "npm run ng build core && npm run ng build animations",
		"build:depth1": "npm run ng build input && npm run ng build scroll && npm run ng build safe-content && npm run ng build number && npm run ng build popover && npm run ng build popup",
		"build:depth2": "npm run ng build tooltip && npm run ng build dropdown && npm run ng build picker && npm run ng build modal",
		"build:depth3": "npm run ng build select && npm run ng build sidepanel && npm run ng build option && npm run ng build date",
		"build:depth4": "npm run ng build api && npm run ng build department && npm run ng build user && npm run ng build establishment",
		"build:material": "npm run ng build material",
		"build:formly": "npm run ng build formly",
		"build:root": "npm run ng build root",
		"postbuild:root": "cpx \"libraries/root/src/style/**\" dist/style",
		"postbuild:material": "cpx \"libraries/material/src/style/**\" dist/material/style",
		"postbuild:formly": "cpx \"libraries/formly/src/style/**\" dist/formly/style",
		"build:schematics": "tsc --project schematics",
		"postbuild:schematics": "npm run link:schematics",
		"link:schematics": "cd schematics && npm link",
		"postlink:schematics": "npm link @lucca-front/schematics",
		"issue": "ng g @lucca-front/schematics:issue"
	},
	"dependencies": {
		"@angular/animations": "^10.0.0",
		"@angular/cdk": "^10.1.0",
		"@angular/common": "^10.0.0",
		"@angular/compiler": "^10.0.0",
		"@angular/core": "^10.0.0",
		"@angular/forms": "^10.0.0",
		"@angular/material": "^10.1.0",
		"@angular/platform-browser": "^10.0.0",
		"@angular/platform-browser-dynamic": "^10.0.0",
		"@lucca-front/icons": "5.0.1",
		"@lucca-front/scss": "5.0.1",
		"@ngx-formly/core": "^5.9.2",
		"core-js": "^3.6.5",
		"rxjs": "^6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "^0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1000.0",
		"@angular-devkit/build-ng-packagr": "^0.1000.0",
		"@angular-devkit/schematics": "^10.0.0",
		"@angular-devkit/schematics-cli": "^0.1000.0",
		"@angular/cli": "^10.0.0",
		"@angular/compiler-cli": "^10.0.0",
		"@angular/material-moment-adapter": "^10.1.0",
		"@angular/router": "^10.0.0",
		"@types/jasmine": "3.5.11",
		"@types/node": "^12.7.11",
		"codelyzer": "^6.0.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "^5.1.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-cli": "~2.0.0",
		"karma-coverage-istanbul-reporter": "^3.0.0",
		"karma-jasmine": "^3.3.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"moment": "^2.27.0",
		"ng-packagr": "^10.0.0",
		"protractor": "^7.0.0",
		"ts-node": "~8.10.2",
		"tsickle": "~0.39.0",
		"tslint": "~6.1.0",
		"typescript": ">=3.9.0 <3.10.0"
	}
}
