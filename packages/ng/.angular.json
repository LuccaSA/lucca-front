{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "applications",
  "projects": {
    "core": {
      "root": "libraries/core",
      "sourceRoot": "libraries/core/src",
      "projectType": "library",
      "prefix": "lu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libraries/core/tsconfig.lib.json",
            "project": "libraries/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libraries/core/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libraries/core/src/test.ts",
            "tsConfig": "libraries/core/tsconfig.spec.json",
            "karmaConfig": "libraries/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libraries/core/tsconfig.lib.json",
              "libraries/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "formly": {
      "root": "libraries/formly",
      "sourceRoot": "libraries/formly/src",
      "projectType": "library",
      "prefix": "lu",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libraries/formly/tsconfig.lib.json",
            "project": "libraries/formly/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libraries/formly/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libraries/formly/src/test.ts",
            "tsConfig": "libraries/formly/tsconfig.spec.json",
            "karmaConfig": "libraries/formly/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libraries/formly/tsconfig.lib.json",
              "libraries/formly/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "root": "docs/demo/",
      "sourceRoot": "docs/demo/src",
      "projectType": "application",
      "prefix": "demo",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "docs/dist/demo",
            "index": "docs/demo/src/index.html",
            "main": "docs/demo/src/main.ts",
            "polyfills": "docs/demo/src/polyfills.ts",
            "tsConfig": "docs/demo/tsconfig.app.json",
            "assets": [
              "docs/demo/src/favicon.ico",
              "docs/demo/src/assets"
            ],
            "styles": [
              "docs/demo/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "docs/demo/src/theme/overrides",
                "libraries/core/src/style/overrides",
                "libraries/core/src/style",
                "node_modules/@lucca-front/scss/src/overrides",
                "node_modules/@lucca-front/ng/src/style/overrides"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "docs/demo/src/environments/environment.ts",
                  "with": "docs/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "docs/demo/src/test.ts",
            "polyfills": "docs/demo/src/polyfills.ts",
            "tsConfig": "docs/demo/tsconfig.spec.json",
            "karmaConfig": "docs/demo/karma.conf.js",
            "styles": [
              "docs/demo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "docs/demo/src/favicon.ico",
              "docs/demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "docs/demo/tsconfig.app.json",
              "docs/demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo-e2e": {
      "root": "docs/demo-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "docs/demo-e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "docs/demo-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "material": {
      "root": "libraries/material",
      "sourceRoot": "libraries/material/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libraries/material/tsconfig.lib.json",
            "project": "libraries/material/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libraries/material/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libraries/material/src/test.ts",
            "tsConfig": "libraries/material/tsconfig.spec.json",
            "karmaConfig": "libraries/material/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libraries/material/tsconfig.lib.json",
              "libraries/material/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "sandbox": {
      "root": "applications/sandbox/",
      "sourceRoot": "applications/sandbox/src",
      "projectType": "application",
      "prefix": "lu",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist-spa/sandbox",
            "index": "applications/sandbox/src/index.html",
            "main": "applications/sandbox/src/main.ts",
            "polyfills": "applications/sandbox/src/polyfills.ts",
            "tsConfig": "applications/sandbox/tsconfig.app.json",
            "assets": [
              "applications/sandbox/src/favicon.ico",
              "applications/sandbox/src/assets"
            ],
            "styles": [
              "applications/sandbox/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libraries/core/src/style/overrides",
                "libraries/core/src/style",
                "node_modules/@lucca-front/scss/src/overrides",
                "node_modules/@lucca-front/ng/src/style/overrides"
              ]
            },
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "applications/sandbox/src/environments/environment.ts",
                  "with": "applications/sandbox/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sandbox:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "applications/sandbox/src/test.ts",
            "polyfills": "applications/sandbox/src/polyfills.ts",
            "tsConfig": "applications/sandbox/tsconfig.spec.json",
            "karmaConfig": "applications/sandbox/karma.conf.js",
            "styles": [
              "applications/sandbox/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "applications/sandbox/src/favicon.ico",
              "applications/sandbox/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "applications/sandbox/tsconfig.app.json",
              "applications/sandbox/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo",
  "schematics": {
    "@lucca-front/schematics:issue": {
      "project": "sandbox"
    }
  }
}