      "@lucca-front/ng/number": [
        "dist/number"
      ],
      "@lucca-front/ng/popover": [
        "dist/popover"
      ],
      "@lucca-front/ng/popup": [
        "dist/popup"
      ],
      "@lucca-front/ng/formly": [
        "dist/formly"
      ],
      "@lucca-front/ng/material": [
        "dist/material"
      ],
      "@lucca-front/ng/tooltip": [
        "dist/tooltip"
      ],
      "@lucca-front/ng/picker": [
        "dist/picker"
      ],
      "@lucca-front/ng/modal": [
        "dist/modal"
