{
  "lerna": "3.4.3",
  "packages": [
    "packages/*",
    "packages/ng/dist"
  ],
  "npmClient": "npm",
  "version": "2.3.0",
  "command": {
    "publish": {
      "message": "publish %s",
      "forcePublish": "*",
      "ignoreChanges": "packages/ng/dist/**",
      "preid": "rc",
      "allowBranch": ["master", "rc"]
    },
    "version": {
      "allowBranch": ["master", "rc"]
    }
  }
}
