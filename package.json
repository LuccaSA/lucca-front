{
	"name": "lucca-front",
	"version": "8.0.0",
	"description": "a library of useful components for web developpement",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LuccaSA/lucca-front.git"
	},
	"keywords": [
		"lucca",
		"angular",
		"ng",
		"scss"
	],
	"author": "lucien bertin <lbertin@lucca.fr>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/LuccaSA/lucca-front/issues"
	},
	"private": true,
	"workspaces": [
		"packages/icons",
		"packages/scss"
	],
	"scripts": {
		"build": "npm run build:icons && npm run build:scss && npm run build:ng",
		"build:icons": "gulp icons",
		"build:scss": "gulp scss",
		"build:ng": "ng run ng:build && gulp ng:schematics",
		"start": "ng run storybook:start",
		"jenkins-test": "jest --no-cache --runInBand",
		"test": "jest --maxWorkers=4 --isolatedModules=true --detectOpenHandles --watchAll",
		"compodoc": "compodoc -p ./tsconfig.compodoc-json.json -e json -d .storybook",
		"build-compodoc": "compodoc -p ./tsconfig.compodoc-html.json -d compodoc-static",
		"build-storybook": "ng run storybook:build",
		"lint": "ng lint"
	},
	"dependencies": {
		"@angular/animations": "13.2.4",
		"@angular/cdk": "^13.2.4",
		"@angular/common": "13.2.4",
		"@angular/compiler": "13.2.4",
		"@angular/core": "13.2.4",
		"@angular/forms": "13.2.4",
		"@angular/platform-browser": "13.2.4",
		"@angular/platform-browser-dynamic": "13.2.4",
		"@angular/router": "13.2.4",
		"@ngx-formly/core": "^5.10.33",
		"cpy": "^8.1.2",
		"cpy-cli": "^3.1.1",
		"normalize.css": "^8.0.1",
		"rxjs": "^7.5.5",
		"tslib": "^2.3.1",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^13.2.5",
		"@angular-eslint/builder": "^13.1.0",
		"@angular-eslint/eslint-plugin": "^13.1.0",
		"@angular-eslint/eslint-plugin-template": "^13.1.0",
		"@angular-eslint/template-parser": "^13.1.0",
		"@angular/cli": "^13.2.5",
		"@angular/compiler-cli": "^13.2.4",
		"@compodoc/compodoc": "^1.1.19",
		"@delagen/eslint-plugin-deprecation": "^1.3.2",
		"@ngneat/spectator": "^10.0.1",
		"@storybook/addon-a11y": "6.4.19",
		"@storybook/addon-actions": "^6.4.19",
		"@storybook/addon-docs": "^6.4.19",
		"@storybook/addon-essentials": "^6.4.19",
		"@storybook/angular": "^6.4.19",
		"@storybook/builder-webpack5": "^6.4.19",
		"@storybook/manager-webpack5": "^6.4.19",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^4.0.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-clean": "^0.4.0",
		"gulp-dart-sass": "^1.0.2",
		"gulp-rename": "^2.0.0",
		"gulp-run": "^1.7.1",
		"gulp-stylelint": "^13.0.0",
		"jest": "^27.5.1",
		"jest-preset-angular": "^11.1.1",
		"ng-packagr": "^13.2.1",
		"postcss": "^8.4.6",
		"postcss-scss": "^4.0.4",
		"postcss-value-parser": "^4.2.0",
		"prettier": "^2.6.2",
		"sass": "^1.50.0",
		"stylelint": "^14.6.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-prettier": "^2.0.0",
		"ts-node": "~10.5.0",
		"typescript": "4.5.5"
	},
	"volta": {
		"node": "14.18.1",
		"npm": "7.24.2"
	}
}
