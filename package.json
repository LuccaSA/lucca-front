{
	"name": "lucca-front",
	"version": "8.0.0",
	"description": "a library of useful components for web developpement",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LuccaSA/lucca-front.git"
	},
	"keywords": [
		"lucca",
		"angular",
		"ng",
		"scss"
	],
	"author": "lucien bertin <lbertin@lucca.fr>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/LuccaSA/lucca-front/issues"
	},
	"private": true,
	"workspaces": [
		"packages/icons",
		"packages/scss"
	],
	"scripts": {
		"build": "npm run build:icons && npm run build:scss && npm run build:ng",
		"build:icons": "gulp icons",
		"build:scss": "gulp scss",
		"build:ng": "ng run ng:build && gulp ng:schematics",
		"start": "ng run storybook:start",
		"compodoc": "compodoc -p ./tsconfig.doc.json -e json -d .storybook",
		"build-storybook": "ng run storybook:build"
	},
	"dependencies": {
		"@angular/animations": "13.0.2",
		"@angular/cdk": "^13.0.2",
		"@angular/common": "13.0.2",
		"@angular/compiler": "13.0.2",
		"@angular/core": "13.0.2",
		"@angular/forms": "13.0.2",
		"@angular/platform-browser": "13.0.2",
		"@angular/platform-browser-dynamic": "13.0.2",
		"@angular/router": "13.0.2",
		"@ngx-formly/core": "^5.10.14",
		"cpy": "^8.1.1",
		"cpy-cli": "^3.1.1",
		"rxjs": "~6.6.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^13.0.3",
		"@angular/cli": "^13.0.3",
		"@angular/elements": "13.0.2",
		"@compodoc/compodoc": "^1.1.11",
		"@storybook/addon-a11y": "6.4.0",
		"@storybook/addon-actions": "^6.4.0",
		"@storybook/addon-docs": "^6.4.0",
		"@storybook/addon-essentials": "^6.4.0",
		"@storybook/addon-knobs": "6.3.0",
		"@storybook/addon-links": "^6.4.0",
		"@storybook/angular": "^6.4.0",
		"@storybook/builder-webpack5": "^6.4.0",
		"@storybook/manager-webpack5": "^6.4.0",
		"@types/node": "^12.11.1",
		"@webcomponents/custom-elements": "^1.4.3",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-clean": "^0.4.0",
		"gulp-dart-sass": "^1.0.2",
		"gulp-rename": "^2.0.0",
		"gulp-run": "^1.7.1",
		"gulp-stylelint": "^13.0.0",
		"ng-packagr": "^13.0.8",
		"postcss": "^8.3.5",
		"sass": "^1.32.11",
		"stylelint": "^13.11.0",
		"ts-node": "~10.0.0",
		"tsconfig-paths-webpack-plugin": "^3.5.2",
		"typescript": "4.4.4"
	},
	"volta": {
		"node": "14.18.1",
		"npm": "7.24.2"
	}
}
